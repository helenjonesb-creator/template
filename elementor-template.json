    "custom_js": "jQuery(document).ready(function($) {\n  $('.dropdown-toggle .elementor-button').on('click', function(e) {\n    e.preventDefault();\n    var additionalServices = $('.additional-services');\n    var button = $(this);\n    var buttonText = button.find('.elementor-button-text');\n    \n    if (additionalServices.hasClass('show')) {\n      additionalServices.removeClass('show');\n      setTimeout(function() {\n        additionalServices.hide();\n      }, 400);\n      buttonText.text('View More Services');\n      button.removeClass('active');\n    } else {\n      additionalServices.show();\n      setTimeout(function() {\n        additionalServices.addClass('show');\n      }, 10);\n      buttonText.text('Show Less Services');\n      button.addClass('active');\n    }\n  });\n});"
